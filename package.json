{
  "name": "@GitHubSuperpower/check-group",
  "version": "1.0.1",
  "description": "A app to group checks based on sub-projects.",
  "author": "Tianhao Zhou <tianhaoz@umich.edu>",
  "license": "ISC",
  "homepage": "https://github.com/tianhaoz95/check-group",
  "bugs": "https://github.com/tianhaoz95/check-group/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/GitHubSuperpower/check-group",
  "scripts": {
    "build": "tsc",
    "start": "probot run ./lib/index.js",
    "start:watch": "tsc && (tsc -w --preserveWatchOutput & nodemon)",
    "start:docs": "docsify serve docs",
    "test": "LOG_LEVEL=fatal jest",
    "test:debug": "jest",
    "remark": "remark .",
    "remark:format": "remark . -o",
    "prettier": "prettier --check .",
    "prettier:format": "prettier --write .",
    "eslint": "eslint src test --ext .ts",
    "eslint:format": "eslint --fix src test --ext .ts",
    "format": "npm run prettier:format && npm run eslint:format",
    "vercel-dev": "vercel dev"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "@octokit/types": "^6.34.0",
    "@octokit/webhooks": "^9.20.0",
    "@sentry/node": "^6.15.0",
    "@sentry/tracing": "^6.14.3",
    "@types/minimatch": "^3.0.5",
    "@types/prettyjson": "0.0.30",
    "dotenv": "^10.0.0",
    "http-status-codes": "^2.1.4",
    "minimatch": "^3.0.4",
    "prettier": "^2.4.1",
    "prettyjson": "^1.2.1",
    "probot": "^12.1.2",
    "vercel": "^23.1.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/nock": "^11.1.0",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "docsify-cli": "^4.4.3",
    "eslint": "^8.3.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^27.4.3",
    "nock": "^13.2.1",
    "nodemon": "^2.0.15",
    "remark": "^14.0.2",
    "remark-cli": "^10.0.1",
    "remark-lint": "^9.1.0",
    "remark-preset-lint-recommended": "^6.1.1",
    "smee-client": "^1.1.0",
    "ts-jest": "^27.0.7",
    "ts-mockito": "^2.6.1",
    "typescript": "^4.5.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
