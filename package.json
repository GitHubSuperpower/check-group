{
  "name": "check-group",
  "version": "1.0.0",
  "private": true,
  "description": "A app to group checks based on sub-projects.",
  "author": "Tianhao Zhou <tianhaoz@umich.edu>",
  "license": "ISC",
  "repository": "https://github.com/tianhaoz95/check-group.git",
  "homepage": "https://github.com/tianhaoz95/check-group",
  "bugs": "https://github.com/tianhaoz95/check-group/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "start": "probot run ./lib/index.js",
    "start:watch": "tsc && (tsc -w --preserveWatchOutput & nodemon)",
    "test": "LOG_LEVEL=fatal jest",
    "test:debug": "jest",
    "remark": "remark .",
    "remark:format": "remark . -o",
    "prettier": "prettier --check .",
    "prettier:format": "prettier --write .",
    "eslint": "eslint src test --ext .ts",
    "eslint:format": "eslint --fix src test --ext .ts",
    "format": "npm run prettier:format && npm run eslint:format"
  },
  "dependencies": {
    "@octokit/rest": "^18.9.1",
    "@octokit/types": "^6.25.0",
    "@octokit/webhooks": "^9.12.0",
    "@sentry/node": "^6.11.0",
    "@sentry/tracing": "^6.11.0",
    "@types/minimatch": "^3.0.5",
    "@types/prettyjson": "0.0.30",
    "dotenv": "^10.0.0",
    "http-status-codes": "^2.1.4",
    "minimatch": "^3.0.4",
    "prettier": "^2.3.2",
    "prettyjson": "^1.2.1",
    "probot": "^12.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/nock": "^11.1.0",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^27.0.6",
    "nock": "^13.1.1",
    "nodemon": "^2.0.12",
    "remark": "^14.0.1",
    "remark-cli": "^10.0.0",
    "remark-lint": "^9.0.0",
    "remark-preset-lint-recommended": "^6.0.1",
    "smee-client": "^1.1.0",
    "ts-jest": "^27.0.5",
    "ts-mockito": "^2.6.1",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
