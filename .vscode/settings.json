{
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "README.md",
        "cmd": "cp README.md docs/README.md"
      },
      {
        "match": "CODE_OF_CONDUCT.md",
        "cmd": "cp CODE_OF_CONDUCT.md docs/code.md"
      },
      {
        "match": "CONTRIBUTING.md",
        "cmd": "cp CONTRIBUTING.md docs/development.md"
      },
      {
        "match": "PRIVACY.md",
        "cmd": "cp PRIVACY.md docs/privacy.md"
      }
    ]
  },
  "cSpell.words": [
    "checkgroup",
    "Codacy",
    "Codecov",
    "codezombiech",
    "dbaeumer",
    "devcontainer",
    "DEVDOCS",
    "docsify",
    "docsmsft",
    "eamodio",
    "emeraldwalk",
    "esbenp",
    "gregorbiswanger",
    "gruntfuggly",
    "interrobang",
    "jsdelivr",
    "kohler",
    "lcov",
    "mockito",
    "octokit",
    "orta",
    "pmneo",
    "prismjs",
    "probot",
    "rbbit",
    "runonsave",
    "smee",
    "socio",
    "stringham",
    "subproj",
    "subproject",
    "subprojects",
    "subprojs",
    "Tianhao",
    "TIANHAOZ",
    "tsimporter",
    "typedoc",
    "vercel",
    "visualstudioexptteam",
    "vscodeintellicode",
    "wayou",
    "wordcount",
    "yzhang",
    "Zhou"
  ],
  "editor.tabSize": 2,
  "markdownlint.config": {
    "MD028": false,
    "MD025": {
      "front_matter_title": ""
    }
  },
  "jest.jestCommandLine": "jest"
}