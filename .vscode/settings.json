{
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "README.md",
        "cmd": "cp README.md docs/README.md"
      },
      {
        "match": "CODE_OF_CONDUCT.md",
        "cmd": "cp CODE_OF_CONDUCT.md docs/code.md"
      },
      {
        "match": "CONTRIBUTING.md",
        "cmd": "cp CONTRIBUTING.md docs/development.md"
      },
      {
        "match": "PRIVACY.md",
        "cmd": "cp PRIVACY.md docs/privacy.md"
      }
    ]
  },
  "cSpell.words": [
    "checkgroup",
    "Codacy",
    "Codecov",
    "docsify",
    "jsdelivr",
    "lcov",
    "mockito",
    "octokit",
    "prismjs",
    "probot",
    "smee",
    "socio",
    "subproj",
    "subprojects",
    "Tianhao",
    "TIANHAOZ",
    "Zhou"
  ],
  "editor.tabSize": 2,
  "markdownlint.config": {
    "MD028": false,
    "MD025": {
      "front_matter_title": ""
    }
  },
  "jest.jestCommandLine": "jest"
}
